// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "shift+cmd+w",
        "command": "-workbench.action.closeWindow"
    },
    {
        "key": "shift+cmd+w",
        "command": "editor.action.rename",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "f2",
        "command": "-editor.action.rename",
        "when": "editorHasRenameProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k ctrl+w",
        "command": "-vscodeWorkspaceSwitcher.openWorkspaceInNewWindow"
    },
    {
        "key": "cmd+u",
        "command": "-cursorUndo",
        "when": "textInputFocus"
    },
    {
        "key": "cmd+u",
        "command": "references-view.find",
        "when": "editorHasReferenceProvider"
    },
    {
        "key": "shift+alt+f12",
        "command": "-references-view.find",
        "when": "editorHasReferenceProvider"
    },
    {
        "key": "ctrl+t",
        "command": "editor.action.refactor",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+r",
        "command": "-editor.action.refactor",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "shift+cmd+t",
        "command": "-workbench.action.reopenClosedEditor"
    },
    {
        "key": "shift+cmd+t",
        "command": "extension.jumpToTest",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+t",
        "command": "-extension.jumpToTest",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+8",
        "command": "-workbench.action.focusEighthEditorGroup"
    },
    {
        "key": "cmd+8",
        "command": "workbench.actions.view.problems"
    },
    {
        "key": "cmd+0",
        "command": "-workbench.actions.view.problems"
    },
    {
        "key": "cmd+0",
        "command": "-workbench.action.focusSideBar"
    },
    {
        "key": "cmd+0",
        "command": "workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "ctrl+`",
        "command": "-workbench.action.terminal.toggleTerminal"
    },
    {
        "key": "cmd+.",
        "command": "-editor.action.quickFix",
        "when": "editorHasCodeActionsProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "cmd+.",
        "command": "editor.action.marker.nextInFiles",
        "when": "editorFocus"
    },
    {
        "key": "f8",
        "command": "-editor.action.marker.nextInFiles",
        "when": "editorFocus"
    },
    {
        "key": "shift+cmd+n",
        "command": "-workbench.action.newWindow"
    },
    {
        "key": "shift+cmd+n",
        "command": "scratchpads.newScratchpad"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.tasks.reRunTask"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.openRecent"
    },
    {
        "key": "ctrl+r",
        "command": "-extension.vim_ctrl+r",
        "when": "editorTextFocus && vim.active && vim.use<C-r> && !inDebugRepl"
    },
    {
        "key": "ctrl+r",
        "command": "-go.test.previous",
        "when": "editorLangId == 'go'"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.quickOpenNavigateNextInRecentFilesPicker",
        "when": "inQuickOpen && inRecentFilesPicker"
    },
    {
        "key": "ctrl+w v",
        "command": "workbench.action.splitEditorRight"
    },
    {
        "key": "ctrl+w h",
        "command": "workbench.action.splitEditorDown"
    },
    {
        "key": "shift+escape",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "cmd+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "shift+cmd+w",
        "command": "renameFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "shift+f6",
        "command": "-renameFile",
        "when": "explorerViewletVisible && filesExplorerFocus"
    },
    {
        "key": "alt+.",
        "command": "editor.action.marker.next",
        "when": "editorFocus"
    },
    {
        "key": "alt+f8",
        "command": "-editor.action.marker.next",
        "when": "editorFocus"
    },
    {
        "key": "cmd+6",
        "command": "workbench.action.output.toggleOutput"
    },
    {
        "key": "shift+cmd+u",
        "command": "-workbench.action.output.toggleOutput"
    },
    {
        "key": "cmd+7",
        "command": "-outline.focus"
    },
    {
        "key": "alt+tab",
        "command": "workbench.action.quickSwitchWindow"
    },
    {
        "key": "alt+tab",
        "command": "-workbench.action.focusNextGroup",
        "when": "editorFocus"
    },
    {
        "key": "alt+r",
        "command": "extension.runJest"
    },
    {
        "key": "alt+w",
        "command": "workbench.action.closePanel"
    },
    {
        "key": "cmd+escape",
        "command": "workbench.action.openRecent"
    },
    {
        "key": "cmd+e",
        "command": "-workbench.action.openRecent"
    },
    {
        "key": "cmd+e",
        "command": "--workbench.action.openRecent"
    },
    {
        "key": "cmd+e",
        "command": "--actions.find"
    },
    {
        "key": "ctrl+cmd+w",
        "command": "-workbench.action.toggleTabsVisibility"
    },
    {
        "key": "ctrl+cmd+w",
        "command": "workbench.action.closeOtherEditors"
    },
    {
        "key": "alt+cmd+t",
        "command": "-workbench.action.closeOtherEditors"
    },
    {
        "key": "cmd+7",
        "command": "outline.focus"
    },
    {
        "key": "cmd+7",
        "command": "-workbench.action.focusSeventhEditorGroup"
    },
    {
        "key": "cmd+6",
        "command": "-workbench.action.focusSixthEditorGroup"
    },
    {
        "key": "cmd+e r",
        "command": "sqltools.executeCurrentQuery"
    },
    {
        "key": "cmd+e cmd+e",
        "command": "-sqltools.executeQuery",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+r",
        "command": "extension.vim_ctrl+r"
    },
    {
        "key": "f5",
        "command": "test-explorer.rerun"
    },
    {
        "key": "shift+f5",
        "command": "test-explorer.run-this-test"
    },
    {
        "key": "shift+f5",
        "command": "-workbench.action.debug.stop",
        "when": "inDebugMode"
    },
    {
        "key": "shift+cmd+f",
        "command": "-workbench.view.search",
        "when": "!searchViewletVisible && config.search.mode == 'view'"
    },
    {
        "key": "shift+cmd+f",
        "command": "-workbench.view.search",
        "when": "!searchViewletVisible"
    },
    {
        "key": "shift+cmd+f",
        "command": "-workbench.action.findInFiles"
    },
    {
        "key": "shift+cmd+f",
        "command": "search.action.openEditor"
    },
    {
        "key": "cmd+9",
        "command": "-workbench.action.toggleSidebarVisibility",
        "when": "!editorFocus"
    },
    {
        "key": "cmd+9",
        "command": "workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm",
        "when": "workbench.scm.active"
    },
    {
        "key": "ctrl+k",
        "command": "git.commitAll"
    },
    {
        "key": "cmd+b",
        "command": "workbench.action.toggleSidebarVisibility"
    }
]